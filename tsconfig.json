{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "paths": {
      "_components/*": ["./src/components/*"],
      "_config/*": ["./src/config/*"],
      "_styles/*": ["./src/styles"],
      "_navigations/*": ["./src/navigations/*"],
      "_screens/*": ["./src/screens/*"],
      "_utils/*": ["./src/utils/*"],
      "_controllers/*":["./src/app/controllers/*"],
      "@env":["./.env"]
    }
  }
}
