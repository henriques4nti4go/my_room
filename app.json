{
  "expo": {
    "scheme": "myroomredirect",
    "name": "MyRoom",
    "slug": "MyRoom",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon_myroom.png",
    "splash": {
      "image": "./assets/splash_myroom.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package":"com.phsa.myroom",
      "versionCode":1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon_myroom.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
